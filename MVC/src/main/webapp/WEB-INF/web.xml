<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
  <servlet>
  	<servlet-name>CoffeeServlet</servlet-name>
  	<display-name>CoffeeServlet</display-name>
  	<description></description>
  	<servlet-class>controller.CoffeeControler</servlet-class>
  </servlet>
 
  <servlet>
  	<servlet-name>CoffeeModel</servlet-name>
  	<display-name>CoffeeModel</display-name>
  	<description></description>
  	<servlet-class>model.CoffeeModel</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>CoffeeServlet</servlet-name>
  	<url-pattern>/coffee</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
  	<servlet-name>CoffeeModel</servlet-name>
  	<url-pattern>/CoffeeModel</url-pattern>
  </servlet-mapping>

</web-app>
